@article{bouchaudClassicalDiffusionParticle1990a,
  title = {Classical Diffusion of a Particle in a One-Dimensional Random Force Field},
  author = {Bouchaud, J. P and Comtet, A and Georges, A and Le Doussal, P},
  year = {1990},
  month = aug,
  journal = {Annals of Physics},
  volume = {201},
  number = {2},
  pages = {285--341},
  issn = {0003-4916},
  doi = {10.1016/0003-4916(90)90043-N},
  abstract = {We present a comprehensive study of the motion of a damped Brownian particle evolving in a static, one dimensional gaussian random force field. We provide both a clear physical picture of the process and a variety of analytical techniques. As the average bias {$\mu$} is increased, a succession of different ``diffusion laws'' is observed: Sinai's diffusion ({$\mu$} = 0, x2 {$\backsimeq$} ln4t), anomalous drift (x {$\backsimeq$} t{$\mu$}, {$\mu$} {$<$} 1), anomalous dispersion (x - Vt {$\backsimeq$} {$\pm$}t1{$\mu$}, 1 {$<$} {$\mu$} {$<$} 2), and finally normal diffusion ({$\mu$} {$>$} 2), apart from algebraic tails outside the scaling region. We show that all those results can be understood in simple terms through a large scale description of the problem as a directed walk among traps characterized by a broad distribution of release time. From this analysis, the full asymptotic probability distributions (averaged over disorder) are precisely determined, in terms of L\'evy stable laws. Sample to sample fluctuations are discussed. The probability of presence at the initial point is more specifically adressed. It amounts to computing the density of states of a Schr\"odinger equation with a special type of random potential. We obtain exactly the average over disorder of this quantity, using the following two different approaches: the ``Dyson-Schmidt'' technique, and the replica method. Both reveal interesting technical features, and the latter can be used to obtain information on the full probability distribution (and Green function). Some physical applications of this model are discussed.},
  langid = {english},
  file = {/home/jacob/snap/zotero-snap/common/Zotero/storage/C4QJVS2V/Bouchaud et al. - 1990 - Classical diffusion of a particle in a one-dimensi.pdf;/home/jacob/snap/zotero-snap/common/Zotero/storage/ZYPDIDP4/000349169090043N.html}
}

