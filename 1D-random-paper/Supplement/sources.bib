
@article{ahmadiDiffusionQuasionedimensionalChannels2017,
  title = {Diffusion in Quasi-One-Dimensional Channels: {{A}} Small System n, p, {{T}}, Transition State Theory for Hopping Times},
  shorttitle = {Diffusion in Quasi-One-Dimensional Channels},
  author = {Ahmadi, Sheida and Bowles, Richard K.},
  year = {2017},
  month = apr,
  journal = {The Journal of Chemical Physics},
  volume = {146},
  number = {15},
  pages = {154505},
  publisher = {{American Institute of Physics}},
  issn = {0021-9606},
  doi = {10.1063/1.4981010},
  abstract = {Particles confined to a single file, in a narrow quasi-one-dimensional channel, exhibit a dynamic crossover from single file diffusion to Fickian diffusion as the channel radius increases and the particles begin to pass each other. The long time diffusion coefficient for a system in the crossover regime can be described in terms of a hopping time, which measures the time it takes for a particle to escape the cage formed by its neighbours. In this paper, we develop a transition state theory approach to the calculation of the hopping time, using the small system isobaric\textendash isothermal ensemble to rigorously account for the volume fluctuations associated with the size of the cage. We also describe a Monte Carlo simulation scheme that can be used to calculate the free energy barrier for particle hopping. The theory and simulation method correctly predict the hopping times for a two-dimensional confined ideal gas system and a system of confined hard discs over a range of channel radii, but the method breaks down for wide channels in the hard discs' case, underestimating the height of the hopping barrier due to the neglect of interactions between the small system and its surroundings.},
  file = {/home/jacob/snap/zotero-snap/common/Zotero/storage/ZKLAAVF4/Ahmadi and Bowles - 2017 - Diffusion in quasi-one-dimensional channels A sma.pdf}
}

@article{albertsIntermediateDisorderRegime2010,
